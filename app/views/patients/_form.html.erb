<%= simple_form_for(@patient) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :issues_mood %>
    <%= f.input :issues_anxiety %>
    <%= f.input :issues_stress %>
    <%= f.input :issues_memory %>
    <%= f.input :issues_headache %>
    <%= f.input :issues_sleep %>
    <%= f.input :issues_weight_loss %>
    <%= f.input :issues_eating_disorder %>
    <%= f.input :issues_focus %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
